  "query": "\n            UPDATE boards\n            SET \n                title = COALESCE($2, title),\n                description = COALESCE($3, description),\n                updated_at = NOW()\n            WHERE id = $1\n            RETURNING id, share_token, title, description, created_at, updated_at\n            ",
